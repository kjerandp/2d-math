{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "one-var": "off",
    "no-mixed-operators": "off",
    "no-restricted-properties": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "one-var-declaration-per-line": "off",
    "import/prefer-default-export": "off"
  }
}
